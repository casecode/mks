<table>
	<thead>
		<tr>
			<th>Video Title</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @videos.each do |video| %>
			<tr>
				<td><%= video.title %></td>
				<td><%= link_to 'Show', video %></td>
				<td><%= link_to 'Edit', edit_video_path(video) %></td>
				<td><%= link_to 'Delete', video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
			</tr>
		<% end %>
	</tbody>
</table>
<br>

<%= link_to 'Create Video', new_video_path %>