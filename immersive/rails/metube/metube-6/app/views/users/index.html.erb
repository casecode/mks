<h1>All the users!</h1>

<ul>

</ul>

<table>
	<thead>
		<tr>
			<th>Name</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
	<% @users.each do |user| %>
		<tr>
			<td><%= link_to user.first_name + " " + user.last_name, user %></td>
			<td><%= user.email_address %></td>
		</tr>
	<% end %>
	</tbody>
</table>
<% if !current_user? %>
<br>
<%= link_to 'Create User', new_user_path %>
<br>
<% end %>